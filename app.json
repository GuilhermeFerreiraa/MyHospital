{
	"expo": {
		"name": "My Hospital",
		"slug": "my-hospital",
		"scheme": "myhospital",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"userInterfaceStyle": "dark",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "cover",
			"backgroundColor": "#2E4A71"
		},
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#2E4A71"
			}
		},
		"web": {
			"favicon": "./assets/favicon.png"
		},
		"experiments": {
      "typedRoutes": true
    },
		"plugins": [
    [
      "expo-router",
      {
        "root": "./src/app/"
      }
    ]
  ]
	}
}
